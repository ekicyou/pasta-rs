{
    "name": "pasta-script",
    "displayName": "pasta-script",
    "description": "pasta script syntax highlighting.",
    "version": "0.0.1",
    "publisher": "ekicyou",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pasta",
                "aliases": [
                    "pasta",
                    "pasta"
                ],
                "extensions": [
                    ".pasta"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pasta",
                "scopeName": "source.pasta",
                "path": "./syntaxes/pasta.tmLanguage.json"
            }
        ]
    }
}