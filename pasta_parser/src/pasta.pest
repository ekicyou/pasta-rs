// token

id      = @{ XID_START ~ XID_CONTINUE* }

spaces  = @{ SP+ }

// MARK, CHAR

SP      = @{ SPACE_SEPARATOR | TAB }
TAB     = @{ "\t" }

AT      = @{ "＠" | "@" }
DOT     = @{ "・" | "･" }
COLON   = @{ "：" | ":" }
CONMA   = @{ "、" |"，" |  "," }
SHARP   = @{ "＃" | "#"| "♯" }
EOL     = @{ "\r\n" | "\n" | "\r" }

// EOF